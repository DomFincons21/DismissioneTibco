<aura:component controller='PED_ProxyWSAsyncServiziTecnici' implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" >
	<aura:attribute name="topic" type="String" default="call.ws.enel.async" access="global" />
    <aura:attribute name="vfBaseURL" type="String" />
    <aura:attribute name="result" type="String" />    
    <aura:attribute name="srcIFrame" type="String" />
    <aura:attribute name="method" type="String" />
    <aura:registerEvent name="PED_CallbackWSAsyncEvent" type="c:PED_CallbackWSAsyncEvent"/>
    <aura:handler  event="c:PED_CallWSAsyncEvent" action="{!c.callWSAsync}"/> 
    
   
    <button id='triggerButton' onclick='{!c.callEventLightning}' style='display:none'/>    
    <p id='result' style='display:none'></p>
    <p id='uniqueID' style='display:none'></p>
    <p id='methodName' style='display:none'></p>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
 	<iframe aura:id="vfFrame" src="{!v.srcIFrame}" frameborder="0" width="1" height="1" />
</aura:component>